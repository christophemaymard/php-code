{
    "name": "cmaymard/php-code",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Christophe Maymard",
            "email": "christophe.maymard@hotmail.com"
        }
    ],
    "keywords": [ "code", "cpp" ],
    "require": {
        "php": ">=7.3",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "8.*"
    },
    "autoload": {
        "psr-4": {
            "PhpCode\\": "src/PhpCode/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhpCode\\Test\\": "test/PhpCode/Test/"
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}

