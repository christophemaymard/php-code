{
    "name": "cmaymard/php-code",
    "description": "Provides support to mangle C++ names.", 
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Christophe Maymard",
            "email": "christophe.maymard@hotmail.com"
        }
    ],
    "keywords": [
        "code", 
        "mangling",
        "cpp" 
    ],
    "require": {
        "php": ">=7.3",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "8.5.*"
    },
    "autoload": {
        "psr-4": {
            "PhpCode\\": "src/PhpCode/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhpCode\\": "test/PhpCode/"
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}

